{
  "name": "remix-todo-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "drizzle:gen": "drizzle-kit generate:sqlite",
    "drizzle:up": "drizzle-kit up:sqlite",
    "local:d1:migrations:list": "wrangler d1 migrations list DB --local",
    "local:d1:migrations:apply": "wrangler d1 migrations apply DB --local"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.29.1",
    "isbot": "^3.7.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-discord": "^1.3.2",
    "remix-validated-form": "^5.1.5",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "wrangler": "3.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
