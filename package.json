{
  "name": "remix-todo-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "drizzle:gen": "drizzle-kit generate:sqlite",
    "drizzle:up": "drizzle-kit up:sqlite",
    "d1:migrations:list": "wrangler d1 migrations list DB",
    "d1:migrations:apply": "wrangler d1 migrations apply DB",
    "local:d1:migrations:list": "wrangler d1 migrations list DB --local",
    "local:d1:migrations:apply": "wrangler d1 migrations apply DB --local"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.0",
    "@conform-to/zod": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.29.3",
    "isbot": "^4.4.0",
    "lucide-react": "^0.321.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-discord": "^1.3.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "@remix-run/dev": "^2.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "wrangler": "3.26.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
